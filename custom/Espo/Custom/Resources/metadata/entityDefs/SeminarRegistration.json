{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "registrationId": {
            "type": "varchar",
            "maxLength": 150,
            "readOnly": true,
            "options": [],
            "isCustom": true
        },
        "membershipId": {
            "type": "int",
            "disableFormatting": true,
            "readOnly": true,
            "isCustom": true
        },
        "registrationDate": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "registrationStatus": {
            "type": "enum",
            "options": [
                "-",
                "Left a Message",
                "Attendance Verified",
                "Cancelled",
                "Rescheduled"
            ],
            "style": {
                "-": null,
                "Left a Message": null,
                "Attendance Verified": null,
                "Cancelled": null,
                "Rescheduled": null
            },
            "default": "Left a Message",
            "isCustom": true
        },
        "attended": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "seminarId2": {
            "type": "int",
            "isCustom": true
        },
        "seminar": {
            "type": "link"
        },
        "contact": {
            "type": "link"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "seminar": {
            "type": "belongsTo",
            "foreign": "seminarRegistrations",
            "entity": "Seminar",
            "audited": false,
            "isCustom": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "seminarRegistrations",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}
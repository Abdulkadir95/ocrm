{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "status": {
            "type": "enum",
            "options": [
                "Planned",
                "Held",
                "Not Held"
            ],
            "default": "Planned",
            "style": {
                "Held": "success"
            },
            "audited": true
        },
        "dateStart": {
            "type": "datetimeOptional",
            "view": "crm:views/meeting/fields/date-start",
            "required": true,
            "default": "javascript: return this.dateTime.getNow(15);",
            "audited": true
        },
        "dateEnd": {
            "type": "datetimeOptional",
            "view": "crm:views/meeting/fields/date-end",
            "required": true,
            "after": "dateStart",
            "suppressValidationList": [
                "required"
            ]
        },
        "isAllDay": {
            "type": "bool",
            "layoutListDisabled": true,
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true
        },
        "duration": {
            "type": "duration",
            "start": "dateStart",
            "end": "dateEnd",
            "options": [
                300,
                600,
                900,
                1800,
                2700,
                3600,
                7200,
                10800
            ],
            "default": 300,
            "notStorable": true,
            "select": {
                "select": "TIMESTAMPDIFF_SECOND:(dateStart, dateEnd)"
            },
            "order": {
                "order": [
                    [
                        "TIMESTAMPDIFF_SECOND:(dateStart, dateEnd)",
                        "{direction}"
                    ]
                ]
            }
        },
        "parent": {
            "type": "linkParent",
            "entityList": [
                "Contact"
            ]
        },
        "description": {
            "type": "text"
        },
        "reminders": {
            "type": "jsonArray",
            "notStorable": true,
            "view": "crm:views/meeting/fields/reminders",
            "layoutListDisabled": true,
            "validatorClassNameList": [
                "Espo\\Modules\\Crm\\Classes\\FieldValidators\\Event\\Reminders\\Valid",
                "Espo\\Modules\\Crm\\Classes\\FieldValidators\\Event\\Reminders\\MaxCount"
            ]
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "seminarRegistration": {
            "type": "link"
        },
        "callStatus": {
            "type": "enum",
            "options": [
                "",
                "Call/Action Completed",
                "Bad #",
                "Bad # / Bad Email",
                "Busy",
                "Answering Machine/No Ans"
            ],
            "style": {
                "": null,
                "Call/Action Completed": null,
                "Bad #": null,
                "Bad # / Bad Email": null,
                "Busy": null,
                "Answering Machine/No Ans": null
            },
            "default": null,
            "isCustom": true
        },
        "callResult": {
            "type": "enum",
            "options": [
                "",
                "None",
                "Left a Message",
                "No Show Called - Created New Sem Reg",
                "No Show Called - Prospect",
                "No Show Called - No Interest",
                "Busy/No Answer",
                "Email Instead"
            ],
            "style": {
                "": null,
                "None": null,
                "Left a Message": null,
                "No Show Called - Created New Sem Reg": null,
                "No Show Called - Prospect": null,
                "No Show Called - No Interest": null,
                "Busy/No Answer": null,
                "Email Instead": null
            },
            "isCustom": true
        }
    },
    "links": {
        "parent": {
            "type": "belongsToParent",
            "foreign": "activityChildren"
        },
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "seminarRegistration": {
            "type": "belongsTo",
            "foreign": "activities",
            "entity": "SeminarRegistration",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "dateStart",
        "order": "desc"
    },
    "indexes": {
        "dateStartStatus": {
            "columns": [
                "dateStart",
                "status"
            ]
        },
        "dateStart": {
            "columns": [
                "dateStart",
                "deleted"
            ]
        },
        "status": {
            "columns": [
                "status",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "assignedUserStatus": {
            "columns": [
                "assignedUserId",
                "status"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}